@model IEnumerable<EnterpriseWebApp.Models.Category>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card mt-5">
    <div class="card-header bg-transparent">
        <h3 class="mb-0">tesr</h3>
    </div>
    <div class="card-body">
        <div class="row icon-examples">
            <div class="col-lg-3 col-md-6">
                @foreach (var item in Model)
                {
                    <ul>
                        <li>
                            @item.CategoryName
                        </li>
                    </ul>
                }
            </div>
        </div>
    </div>
</div>